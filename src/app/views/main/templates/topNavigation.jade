.site-navigation(data-spy='affix', data-offset-top='450')
	.sidebar-display(data-ng-class="{ 'open': catCtrl.sidebarToggle.left === true }")
		#menu-trigger(data-target='mainmenu' data-toggle-sidebar data-model-left='catCtrl.sidebarToggle.left' data-model-layout-type='catCtrl.layoutType')
			.line-wrap
				.line.top
				.line.center
				.line.bottom
		.filter-display {{ 'toggleFilterMenu' | translate }}
	.right-display
		.item-count.align-right
			img(src='images/ico-right.png')
			span(data-ng-bind-html="'totalFoundSheets' | translate:'{current: '+catCtrl.registersData.length+', total: '+catCtrl.totalRegisters+'}'")
