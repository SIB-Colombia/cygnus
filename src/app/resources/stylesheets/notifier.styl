[data-notify="container"][class*="alert-pastel"]
	background-color #00708A
	border-width 0px
	border-radius 10px
	box-shadow 0px 0px 5px rgba(51, 51, 51, 0.3)
	//width 300px
	padding 10px 15px 10px 15px
	& > [data-notify="title"]
		color #42F2DD
		display block
		font-weight 700
		margin-bottom 5px
	& > [data-notify="message"]
		font-weight 400
		color white
		margin-bottom 5px
		display block
